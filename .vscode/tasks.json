{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "sweetpad",
			"action": "build",
			"problemMatcher": [
				"$sweetpad-watch",
				"$sweetpad-xcodebuild-default",
				"$sweetpad-xcbeautify-errors",
				"$sweetpad-xcbeautify-warnings"
			],
			"label": "Build",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
        "reveal": "silent"
			}
		}
	]
}